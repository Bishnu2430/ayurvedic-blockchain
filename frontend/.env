# API Configuration
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_BLOCKCHAIN_API_URL=http://localhost:3002/api

# App Configuration
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_DATE=2024-01-15
REACT_APP_GIT_COMMIT=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_ENABLE_DEV_TOOLS=true

# External Services (Optional - add your keys when needed)
# REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
# REACT_APP_ANALYTICS_ID=your_analytics_id_here
# REACT_APP_SENTRY_DSN=your_sentry_dsn_here

# Development Settings
GENERATE_SOURCEMAP=true
FAST_REFRESH=true

# Build Settings
BUILD_PATH=build
INLINE_RUNTIME_CHUNK=false

# Browser Settings
BROWSER=default
HTTPS=false

# Development Port (Optional - defaults to 3000)
# PORT=3000